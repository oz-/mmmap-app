{
  "name": "@mmmap/app",
  "version": "0.5.0-alpha.537",
  "private": true,
  "description": "The mmmap application, to connect to the MARCEL multimedia art platform.",
  "author": "Beno√Æt LAHOZ <benoit.lahoz@carasuelo.org>",
  "scripts": {
    "lint": "vue-cli-service lint",
    "electron:generate-icons": "electron-icon-builder --input=./src/gui/public/icon_2020.png --output=build --flatten",
    "electron:build": "node update.build.js && NODE_ENV=production vue-cli-service --mode production electron:build --mac --linux deb --win nsis --publish never",
    "electron:deploy": "node update.build.js && NODE_ENV=production vue-cli-service --mode production electron:build --mac --linux deb --win nsis --publish always",
    "electron:serve": "mkdir -p ./dist_electron && cp ./copy/dev-app-update.yml ./dist_electron/dev-app-update.yml && node update.build.js && vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "mmmap:upgrade": "yarn upgrade @vuoz/components @vuoz/theme-core-default && yarn add --dev @types/serialport"
  },
  "productName": "mmmap",
  "main": "background.js",
